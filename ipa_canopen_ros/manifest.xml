<package>
  <description brief="ipa_canopen_ros">

     ROS node to communicate with CANopen devices.
     User manual: https://github.com/ipa320/ipa_canopen/blob/master/ipa_canopen_core/doc/usermanual.pdf?raw=true

  </description>
  <author>Tobias Sing</author>
  <author>Thiago de Freitas</author>
  <maintainer email="tdf@ipa.fhg.de">Thiago de Freitas</maintainer>
  <license>LGPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/ipa_canopen_ros</url>
  <depend package="std_msgs"/>
  <depend package="control_msgs"/>
  <depend package="diagnostic_msgs"/>
  <depend package="brics_actuator"/>
  <depend package="rospy"/>
  <depend package="urdf"/>
  <depend package="roscpp"/>
  <depend package="libpcan"/>
  <depend package="cob_srvs"/> 
  <depend package="sensor_msgs"/> 
  <depend package="ipa_canopen_core"/>
  
  <rosdep name="yaml-cpp"/>

  <export>
    <cpp cflags="-I${prefix}/include"/>
  </export>

</package>
